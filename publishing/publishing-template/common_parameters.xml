<parameter name="webhelp.show.main.page.tiles" value="yes"/>
<parameter name="webhelp.show.main.page.toc" value="no"/>

<parameter name="webhelp.top.menu.depth" value="3"/>
<parameter name="use.stemming" value="false"/>
<parameter name="webhelp.show.child.links" value="yes"/>

<!-- Generate complete TOC -->
<parameter name="webhelp.side.toc.links" value="all"/>

<!-- Set default collection-type to sequence -->
<parameter name="webhelp.default.collection.type.sequence" value="yes"/>

<!-- Do not merge related links in chunk topics -->
<parameter name="webhelp.merge.nested.topics.related.links" value="no"/>

<parameter name="webhelp.pdf.link.text" value="PDF user guide"/>

<!-- Generate Google Structured Data in WebHelp output -->
<parameter name="google.structured.data" value="yes"/>

<!-- Enable dark mode -->
<parameter name="webhelp.enable.dark.mode" value="yes"/>

<!-- Enable AI friendly -->
<parameter name="webhelp.enable.ai.friendly" value="yes"/>
