          <dlentry id="dlentry_wck_cgk_54b">
            <dt><uicontrol>Text</uicontrol> submenu</dt>
            <dd>
              <p id="p_xck_cgk_54b">This submenu contains the following actions:</p>
              <p id="p_yck_cgk_54b">
                <dl id="ul_ckk_ftc_gd">
                  <dlentry id="dlentry_zck_cgk_54b">
